{
  "env": {
    "electron": {
      "presets": [
        ["env", {
          "targets": { "electron": "2.0" },
          "useBuiltIns": true
        }], "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-builtin-extend", { "globals": ["Error"] }]
      ]
    },
    "development": {
      "presets": ["react-native", ["transform-builtin-extend", { "globals": ["Error"] }]]
    }
  }
}
